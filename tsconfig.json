{
  "compilerOptions": {
    "baseUrl": "./",
    "strictNullChecks": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "jsx": "preserve",
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noImplicitAny": false,
    "target": "es6",
    "lib": ["dom", "es2017","esnext"],
    "skipLibCheck": true,
    "paths": {
      "etable": [
        "src/index.tsx"
      ]
    }
  },
  "include": ["src", "typings"],
  "exclude": ["node_modules", "lib", "es"]
}
